/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@counter/*": [
        "src/app/feature/counter/*"
      ],
      "@pokemones/*": [
        "src/app/feature/pokemones/*"
      ],
      "@father/*": [
        "src/app/feature/father/*"
      ],
      "@father-son/*": [
        "src/app/feature/father-son/*"
      ],
      "@counter-route/*": [
        "src/app/feature/counter-route/*"
      ],
      "@shared/*":[
        "src/app/shared/*"
      ]
    },
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
